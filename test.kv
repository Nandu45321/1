<TriangleWindow>:
    name:"Triangle"
    BoxLayout:
        orientation:'vertical'
        MDTopAppBar:
            title: "Triangle"
            left_action_items: [['arrow-left', lambda x: root.manager.change_screen_for_shapes("MainScreen")]]
        BoxLayout:
            orientation:'vertical'
            padding: '3dp'
            spacing: '10dp'
            ScrollView:
                always_overscroll:False
                do_scroll_x:False
                size_hint: 1,2
                GridLayout:
                    size_hint_y:None
                    height:self.minimum_height
                    size:self.minimum_size
                    spacing:dp(10)
                    cols:1
                    MDGridLayout:
                        row_default_height:dp(10)
                        size_hint_y:None
                        height:self.minimum_height
                        size:self.minimum_size
                        cols:2
                        MDLabel:
                            text: 'Base  :'
                            size_hint_y: None
                            height:dp(60)
                            halign: "center"
                            width: dp(85)
                        MDTextField:
                            id: base_value
                            size_hint_y:None
                            color_mode: "accent"
                            halign: "auto"
                            multiline: False
                            input_type: "number"
                            input_filter: "float"
                            on_text_validate: root.queans()
                        MDLabel:
                            text: 'Height  :'
                            size_hint_y: None
                            height:dp(60)
                            halign: "center"
                            width: dp(85)
                        MDTextField:
                            id: height_value
                            size_hint_y:None
                            color_mode: "accent"
                            halign: "auto"
                            multiline: False
                            input_type: "number"
                            input_filter: "float"
                            on_text_validate: root.queans()
            BoxLayout:
                size_hint: 1, 0.4
                orientation: "horizontal"
                MDLabel:
                MDFloatingActionButton:
                    icon: "arrow-down"
                    pos_hint: {'x': .45,  'center_y': .5}
                    on_release: root.queans()
                MDLabel:
            ScrollView:
                always_overscroll:False
                do_scroll_x:False
                size_hint: 1,2
                GridLayout:
                    size_hint_y:None
                    height:self.minimum_height
                    size:self.minimum_size
                    spacing:dp(10)
                    cols:1
                    MDGridLayout:
                        row_default_height:dp(75)
                        size_hint_y:None
                        height:self.minimum_height
                        size:self.minimum_size
                        cols:2
                        MDLabel:
                            text: 'Area  is'
                            size_hint_y: None
                            height:dp(60)
                            halign: "center"
                            width: dp(85)
                        MDTextField:
                            id: area_value
                            size_hint_y:None
                            color_mode: "accent"
                            halign: "auto"
                            multiline: False
                            disabled: True
                        MDLabel:
                            text: 'Perimeter  is'
                            size_hint_y: None
                            height:dp(60)
                            halign: "center"
                            width: dp(85)
                        MDTextField:
                            id: perimeter_value
                            size_hint_y:None
                            color_mode: "accent"
                            halign: "auto"
                            multiline: False
                            disabled: True
